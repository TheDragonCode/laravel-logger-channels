{
    "name": "andrey-helldar/laravel-logger-channels",
    "description": "Set of custom loggers for Monolog in Laravel",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "logger",
        "custom logger",
        "custom channels",
        "custom monolog"
    ],
    "authors": [
