{
    "name": "andrey-helldar/laravel-logger-channels",
    "description": "Set of custom loggers for Monolog in Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["logger", "custom logger", "custom channels", "custom monolog"],
    "support": {
        "issues": "https://github.com/andrey-helldar/laravel-logger-channels/issues",
        "source": "https://github.com/andrey-helldar/laravel-logger-channels"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Helldar\\LaravelLoggerChannels\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
