        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "monolog/monolog": "^2.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Helldar\\LaravelLoggerChannels\\": "src"
        },
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true
        },
        "preferred-install": "dist",
        "sort-packages": true
